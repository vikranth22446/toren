    "log_pattern": "/root/.claude/projects/*.jsonl",
    "pricing": {
      "input": 0.000003,
      "output": 0.000015
    }
  },
  "gpt": {
    "command": "gpt-cli",
    "args": ["--no-confirm"],
    "env_vars": [],
    "log_pattern": "/tmp/gpt_logs/*.json",
